{
  "name": "center-management-electron",
  "version": "0.1.0",
  "description": "Center Management System - Electron Desktop App",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm run dev:server\" \"npm run dev:renderer\" \"npm run dev:main\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:renderer": "vite",
    "dev:main": "tsc -w --project tsconfig.main.json",
    "dev:server": "tsx watch server/index.ts",
    "build": "npm run build:renderer && npm run build:main && npm run build:preload",
    "build:renderer": "vite build",
    "build:main": "tsc --project tsconfig.main.json",
    "build:preload": "tsc --project tsconfig.preload.json",
    "start": "electron .",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.35.0",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "electron-updater": "^6.1.7",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "jose": "^6.1.0",
    "jspdf": "^3.0.3",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.479.0",
    "mongodb": "^6.20.0",
    "motion": "^12.23.12",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "^6.28.0",
    "react-schemaorg": "^2.0.0",
    "i18next": "^23.7.6",
    "react-i18next": "^15.0.3",
    "recharts": "^3.2.1",
    "schema-dts": "^1.1.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.13",
    "@types/qrcode": "^1.5.6",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.5.6",
    "prisma": "^6.16.2",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.7.1",
    "typescript": "^5.9.2",
    "vite": "^5.4.0"
  },
  "build": {
    "appId": "com.center.management",
    "productName": "Center Management",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "server/**/*",
      "prisma/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": ["nsis"],
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": ["dmg"],
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": ["AppImage"],
      "icon": "build/icon.png"
    }
  }
}

